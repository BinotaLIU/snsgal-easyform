{
    "name": "snsgal/yubin-terminator",
    "description": "Yubin terminator, the last application you will need to request an address.",
    "keywords": ["snsgal", "address", "shipping", "form"],
    "license": "GPL",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "doctrine/dbal": "^2.7",
        "elfsundae/laravel-hashid": "^1.3",
        "fideloper/proxy": "~4.0",
        "greg0ire/enum": "^3.3",
        "guzzlehttp/guzzle": "^6.4",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^6.6.0",
        "laravel/tinker": "^2.0",
        "laravel/vapor-core": "^2.2",
        "ramsey/uuid": "^3.5",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "filp/whoops": "~2.0",
        "barryvdh/laravel-ide-helper": "^2.6",
        "barryvdh/laravel-debugbar": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-autoload-dump": [
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "ide": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
