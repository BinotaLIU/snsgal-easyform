{
    "name": "snsgal/yubin-terminator",
    "description": "Yubin terminator, the last application you will need to request an address.",
    "keywords": ["snsgal", "address", "shipping", "form"],
    "license": "GPL",
    "type": "project",
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.1",
        "elfsundae/laravel-hashid": "^1.6",
        "fideloper/proxy": "^4.4",
        "greg0ire/enum": "^4.3",
        "guzzlehttp/guzzle": "^7.2",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^8.0.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravel/vapor-core": "^2.10",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^2.3",
        "fakerphp/faker": "^1.13",
        "mockery/mockery": "1.4.*",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-latest",
        "symfony/css-selector": "5.2.*",
        "symfony/dom-crawler": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories",
            "Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-autoload-dump": [
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "ide": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
